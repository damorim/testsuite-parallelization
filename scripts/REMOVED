else:
            ## download the project if it satisfies the criteria
            git_cmd = "git clone http://github.com/{} -b {} --depth {} --recursive {}".format(full_name, branch, 50, output_dir)
            msgLogger.warning("cloning project {}".format(full_name))
            call(git_cmd, shell=True)
            msgLogger.warning("done cloning")
            
===

                else:
                    ## project is interesting and was cloned
                    entry["rev"] = git_revision(project_path)
                    has_maven_support = verify_maven_support(project_path)
                    entry["ismaven"] = has_maven_support
                    output.write(entry)
                    msgLogger.warning("Writing in CSV !!")

                    if os.path.exists(project_path) and not has_maven_support:                        
                        msgLogger.error("[FATAL ERROR] This should never happen!!!!!!!!!!!!!!!!")
                        rmtree(project_path)
                    else:
                        ## I don't understand. At this point, you should compile and test          
                        compile_cmd = "./compile-test-new.sh 90m ./downloads ./raw_data"
                        call(compile_cmd, shell=True)
